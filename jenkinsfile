    agent any
    stages {
        stage('build') {
            steps {
                echo 'building the applications'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying the application'
            }
        }
        stage('test') {
            steps {
                echo 'testing the apllication'
            }
        }
        stage('release') {
            steps {
                echo 'releasing the applications'
            }
        }
    }
}
